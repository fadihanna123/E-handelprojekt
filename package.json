  "scripts": {
    "setup": "yarn --emoji --silent --ignore-scripts",
    "dev": "live-server .",
    "type-check": "tsc --noEmit",
    "lint": "eslint . && stylelint css/*.css && yarn type-check",
    "format": "prettier . -w",
    "clear": "rimraf node_modules",
    "build": "esbuild ts/* --minify --outdir=js",
    "deploy": "yarn setup && gh-pages -d .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jquery": "^3.5.32",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "htmlhint": "^1.1.4",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "stylelint": "^16.13.2",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "homepage": "https://fadihanna123.github.io/E-handelprojekt"
